{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "postinstall": "node generate-proto.js",
    "generate-proto": "node generate-proto.js",
    "clean-proto": "find src/proto -name '*_pb.*' -delete && node generate-proto.js",
    "clean-proto-win": "del /S /Q src\\proto\\*_pb.* && node generate-proto.js"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.12.5",
    "@protobuf-ts/grpc-transport": "^2.9.4",
    "@protobuf-ts/grpcweb-transport": "^2.9.4",
    "@protobuf-ts/runtime": "^2.9.4",
    "@tonalflex/template-plugin": "^0.0.8",
    "@tonalflex/neuralamp-plugin": "^0.0.9",
    "@tonalflex/delay-plugin": "^0.0.2",
    "google-protobuf": "^3.21.4",
    "grpc-tools": "^1.12.4",
    "grpc-web": "^1.5.0",
    "oh-vue-icons": "^1.0.0-rc3",
    "protobufjs": "^7.4.0",
    "protoc-gen-ts": "^0.8.7",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5",
    "pinia": "^3.0.2"
  },
  "devDependencies": {
    "@eslint/css": "^0.6.0",
    "@eslint/js": "^9.24.0",
    "@eslint/json": "^0.11.0",
    "@eslint/markdown": "^6.3.0",
    "@protobuf-ts/plugin": "^2.9.4",
    "@tsconfig/node22": "^22.0.0",
    "@types/google-protobuf": "^3.15.12",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.24.0",
    "eslint-plugin-vue": "^9.33.0",
    "glob": "^11.0.1",
    "globals": "^16.0.0",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "protoc-gen-grpc-web": "^1.5.0",
    "sass": "^1.83.1",
    "sass-loader": "^16.0.4",
    "ts-proto": "^2.7.0",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.0",
    "vite-plugin-vue-devtools": "^7.6.5",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.2.4"
  }
}
